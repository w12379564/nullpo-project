[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=mem001
Group=www-data
WorkingDirectory=/home/mem001/~sites/fgdemo
ExecStart=/home/mem001/~sites/fgdemo/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/tmp/nullpo.sock webproject.wsgi:application

[Install]
WantedBy=multi-user.target